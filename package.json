{
    "name": "wonderlandsnippets",
    "repository": {
        "type": "github",
        "url": "https://github.com/sorskoot/WonderlandSnippets"
    },
    "displayName": "Wonderland Snippets",
    "description": "Code snippets for use with the Wonderland Engine",
    "version": "1.1.8",
    "publisher": "Sorskoot",
    "icon": "wonderland.png",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/wonderland.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/wonderland-ts.code-snippets"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/wonderland-ts.code-snippets"
            }
        ]
    },
    "scripts": {
        "vsce:package": "vsce package",
        "vsce:publish": "vsce publish"
    },
    "devDependencies": {
        "@vscode/vsce": "^3.1.0"
    }
}
